class Node {
	Object obj;
	Node right;
	Node left;
	public Node(Object o) {
		obj = o;
	}
}

public class StorageBox {
	Node stack, head;
	public StorageBox() {
		stack = new Node(null);
		head = stack;
	}
	public boolean isEmpty() {
		return head == stack;
	}
	public void push(Object x) {
		head.right = new Node(x);
	}
	public void pop() {
		if(head != stack) {
			
		}
	}
	public Object top() {
		if(head == stack) {
			return null;
		} else {
			return head.obj;
		}
	}
}
